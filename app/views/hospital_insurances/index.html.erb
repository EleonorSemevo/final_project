<p id="notice"><%= notice %></p>

<h1>Hospital Insurances</h1>

<table>
  <thead>
    <tr>
      <th>Hospital</th>
      <th>Insurance</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hospital_insurances.each do |hospital_insurance| %>
      <tr>
        <td><%= hospital_insurance.hospital %></td>
        <td><%= hospital_insurance.insurance %></td>
        <td><%= link_to 'Show', hospital_insurance %></td>
        <td><%= link_to 'Edit', edit_hospital_insurance_path(hospital_insurance) %></td>
        <td><%= link_to 'Destroy', hospital_insurance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Hospital Insurance', new_hospital_insurance_path %>
