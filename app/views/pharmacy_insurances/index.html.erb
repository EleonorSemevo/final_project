<p id="notice"><%= notice %></p>

<h1>Pharmacy Insurances</h1>

<table>
  <thead>
    <tr>
      <th>Pharmacy</th>
      <th>Insurance</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pharmacy_insurances.each do |pharmacy_insurance| %>
      <tr>
        <td><%= pharmacy_insurance.pharmacy %></td>
        <td><%= pharmacy_insurance.insurance %></td>
        <td><%= link_to 'Show', pharmacy_insurance %></td>
        <td><%= link_to 'Edit', edit_pharmacy_insurance_path(pharmacy_insurance) %></td>
        <td><%= link_to 'Destroy', pharmacy_insurance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pharmacy Insurance', new_pharmacy_insurance_path %>
